{
  "Application": {
    "DefaultLayout": "Default",
    "NotFoundController" : "Home",
    "NotFoundAction": "NotFound"
  },
  "Debug":{
    "DieOnRoutingError": true,
    "DontCacheModels": false
  },
  "Logging": {
    "FileDirectory": "/logs/",
    "Loggers": [
      {
        "CacheLogger": {
          "Name": "Cache"
        }
      }
    ]
  },
  "Caching": {
    "MemcachedOutputCache": {
      "Name": "OutputCache",
      "ServerHost": "127.0.0.1",
      "ServerPort": 11211,
      "StorageTime": 10
    }
  }
}