{
  "Application": {
    "DefaultLayout": "Default",
    "NotFoundController" : "Home",
    "NotFoundAction": "NotFound"
  },
  "Debug":{
    "DieOnRoutingError": true,
    "DontCacheModels": false
  },
  "Logging": {
    "FileDirectory": "/logs/",
    "Loggers": [
      {
        "CacheLogger": {
          "Name": "Cache"
        }
      }
    ]
  },
  "Caching": {
    "FileOutputCache": {
      "Name": "OutputCache",
      "CacheFolder": "/Temp/Cache/Output/",
      "StorageTime": 900
    }
  }
}